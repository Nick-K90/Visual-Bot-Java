<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<BorderPane prefHeight="500.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ogame.bot.MainSceneController">
   <center>
        <Pane BorderPane.alignment="CENTER">
            <children>
            <TabPane prefHeight="500.0" prefWidth="650.0" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                <Tab text="Untitled Tab 1">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <ListView fx:id="targetImageListView" layoutX="401.0" layoutY="103.0" prefHeight="220.0" prefWidth="177.0" />
                                  <Button fx:id="screenshotButton" layoutX="8.0" layoutY="43.0" mnemonicParsing="false" text="Take Screenshot" />
                                  <Button fx:id="resultButton" layoutX="111.0" layoutY="43.0" mnemonicParsing="false" text="Print Results" />
                                  <TextArea fx:id="textArea" layoutX="8.0" layoutY="102.0" prefHeight="355.0" prefWidth="377.0" />
                              <Button fx:id="startButton" layoutX="135.0" layoutY="14.0" mnemonicParsing="false" text="Start" />
                              <ImageView fx:id="screenshotImageView" fitHeight="99.0" fitWidth="177.0" layoutX="401.0" layoutY="6.0" pickOnBounds="true" preserveRatio="true" />
                              <Button fx:id="analyzeButton" layoutX="8.0" layoutY="14.0" mnemonicParsing="false" text="Analyze" />
                              <Button fx:id="clearTextButton" layoutX="66.0" layoutY="14.0" mnemonicParsing="false" text="Clear Text" />
                              <Button fx:id="stopButton" layoutX="176.0" layoutY="14.0" mnemonicParsing="false" text="Stop" />
                              <ImageView fx:id="targetImageView" fitHeight="122.0" fitWidth="177.0" layoutX="401.0" layoutY="335.0" pickOnBounds="true" preserveRatio="true" />
                              <Button fx:id="refreshListButton" layoutX="193.0" layoutY="43.0" mnemonicParsing="false" text="Refresh List" />
                              <Label fx:id="mouseXPosLabel" layoutX="14.0" layoutY="85.0" text="Mouse X: " />
                              <Label fx:id="mouseYPosLabel" layoutX="101.0" layoutY="85.0" text="Mouse Y: " />
                           </children>
                        </AnchorPane>
                  </content>
                </Tab>
                <Tab text="Untitled Tab 2">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                  </content>
                </Tab>
              </tabs>
            </TabPane>
            </children>
        </Pane>
   </center>
</BorderPane>
